module Main where

import Siko.Parser

source = do
        lines <- ["module Main.Foo where",
                  "main = ",
                  "   . println",
                  "main2 = ",
                  "   println",
                  ]
        (_, src) <- loop (0, "") \(count, src) -> if count < getLength lines
                        then
                            do
                                line <- atIndex lines count
                                count <- count + 1
                                src <- src + "\n" + line
                                ((count, src), True)
                        else
                            ((count, src), False)
        src

main = do
    println "Sikoc experiment"
    program <- parse source
    ()